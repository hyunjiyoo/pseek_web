<!-- Navigation -->
<nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
    <div class="container">
        <a class="navbar-brand js-scroll-trigger" href="/">P-Seek :: <small>artists' playground</small></a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            Menu
            <i class="fas fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
            <ul class="navbar-nav text-uppercase ml-auto">
                <span hidden>
                <%= artGenre = ['abstract', 'contemporary', 'modern', 'pop', 'stillLife', 'surrealism' ] %>
                <%= artName = ['추상화', '현대미술', '모던아트', '팝아트', '정물화', '초현실주의'] %>
                </span>
                <% for(let i in artGenre) { %>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="./<%= artGenre[i] %>"><%= artName[i] %></a>
                    </li>
                <% } %>
                <li class="nav-item" style="margin-top: 13px;">
                    <% if (isLogined) { %>
                        <i class="fas fa-user-circle" style="color: #fed136; padding-left: 5px"></i>
                        <span id="user" style="color: #fed136; padding-left: 5px"><%= user %></span>
                    <% } else { %>
                        <i class="fas fa-user-circle" style="color: #ADADAD;  padding-left: 5px"></i>
                        <span id="user" style="color: #fed136; padding-left: 5px">Guest</span>
                    <% } %>
                </li>
            </ul>
        </div>
    </div>
</nav>


