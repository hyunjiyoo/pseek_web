<!-- Navigation -->
<nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
    <div class="container">
        <a class="navbar-brand js-scroll-trigger" href="/">P-Seek :: <small>artists' playground</small></a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            Menu
            <i class="fas fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
            <ul class="navbar-nav text-uppercase ml-auto">
                <span hidden>
                <%= artGenre = ['contemporary', 'modern', 'pop', 'abstract', 'surrealism', 'stillLife'] %>
                    <%= artName = ['현대미술', '모던아트', '팝아트', '추상화', '초현실주의', '정물화'] %>
                </span>
                <% for(let i in artGenre) { %>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="<%= artGenre[i] %>"><%= artName[i] %></a>
                    </li>
                <% } %>
                <li class="nav-item" style="margin-top: 10px;">
                    <a class="btn btn-primary btn-sm" href="#" style="color: #474747">Sign In</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

